{
  "name": "vader-language-support",
  "displayName": "Vader Language Support",
  "description": "ðŸš€ Programa en espaÃ±ol natural - El primer lenguaje universal multiidioma del mundo. Syntax highlighting, snippets y temas incluidos.",
  "version": "1.0.1",
  "publisher": "vader-lang",
  "author": {
    "name": "Vader Team",
    "email": "team@vader-lang.org"
  },
  "homepage": "https://github.com/LangVader/core",
  "repository": {
    "type": "git",
    "url": "https://github.com/LangVader/core.git"
  },
  "bugs": {
    "url": "https://github.com/LangVader/core/issues"
  },
  "license": "MIT",
  "icon": "../assets/logo.png",
  "readme": "README.md",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages",
    "Themes",
    "Snippets"
  ],
  "keywords": [
    "vader",
    "espaÃ±ol",
    "spanish",
    "programming",
    "natural language",
    "transpiler",
    "multiidioma",
    "universal"
  ],
  "activationEvents": [
    "onLanguage:vader"
  ],
  "contributes": {
    "languages": [
      {
        "id": "vader",
        "aliases": [
          "Vader",
          "vader"
        ],
        "extensions": [
          ".vdr"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "../assets/logo.png",
          "dark": "../assets/logo.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "vader",
        "scopeName": "source.vader",
        "path": "./syntaxes/vader.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "vader",
        "path": "./snippets/vader.json"
      }
    ],
    "themes": [
      {
        "label": "Vader Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/vader-dark.json"
      },
      {
        "label": "Vader Light",
        "uiTheme": "vs",
        "path": "./themes/vader-light.json"
      }
    ]
  }
}
